<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Projeto Financeiro - Leonardo Maran</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <style>
        .navbar-header button {
            color: #fff;
        }

        .navbar-header button {
            color: #fff;
        }

        .fullscreen {
            width: 100%;
        }

        main {
            padding-top: 50px;
        }
    </style>
</head>
<body>
    @Html.Partial("_MenuSuperior")
    <div class="container">
        <main class="col-sm-8">
            @RenderBody()
        </main>
        <div class="col-sm-4">
            <h3>Acesso Rápido</h3>
            <ul>
                <li>@Html.ActionLink("Movimentações por Usuário", "MovimentacoesPorUsuario", "Movimentacao")</li>
                <li>@Html.ActionLink("Busca de movimentações","Busca", "Movimentacao")</li>
            </ul>

        </div>
    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
</body>
</html>